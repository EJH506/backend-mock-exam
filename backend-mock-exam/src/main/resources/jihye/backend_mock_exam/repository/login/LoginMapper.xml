<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jihye.backend_mock_exam.repository.login.LoginMapper">
    <select id="findByLoginData" resultType="user">
        SELECT * FROM users
        WHERE account_id = #{loginId}
        AND password_hash = #{loginPassword};
    </select>
</mapper>